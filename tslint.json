{
  "extends": [
    "tslint:latest",
    "tslint-react",
    "tslint-immutable",
    "tslint-eslint-rules"
  ],
  "rules": {
    "quotemark": [true, "single"],
    "semicolon": [true],
    "jsx-wrap-multiline": false,
    "object-literal-sort-keys": false,
    "ordered-imports": false,
    "no-console": [true, "log"],
    "jsx-no-multiline-js": false
  }
}
